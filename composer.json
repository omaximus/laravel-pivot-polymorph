{
  "name": "omaximus/laravel-pivot-polymorph",
  "description": "Fully polymorphic pivot relations for laravel eloquent",
  "keywords": ["laravel", "relations", "polymorphic"],
  "require": {
    "php": ">=7.1.3|^8",
    "illuminate/database": "^6.0|^7|^8|^9"
  },
  "autoload": {
    "psr-4": {
      "Pisochek\\PivotPolymorph\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pisochek\\PivotPolymorph\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
